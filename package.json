{
  "name": "vue-simple-acl",
  "version": "1.1.2",
  "description": "A simple unopinionated Vue plugin for managing user roles and permissions, access-control list (ACL) and role-based access control (RBAC).",
  "main": "dist/vue-simple-acl.js",
  "browser": "dist/vue-simple-acl.min.js",
  "module": "dist/vue-simple-acl.esm.js",
  "unpkg": "dist/vue-simple-acl.min.js",
  "jsdelivr": "dist/vue-simple-acl.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production npm run build:package",
    "build:vite": "vuedx-typecheck . && vite build",
    "serve": "vite preview",
    "build:package": "rollup --config rollup.config.js",
    "deploy": "npm lint & npm build & npm publish"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.7",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "axios": "^0.21.1",
    "eslint": "^7.21.0",
    "rollup": "^2.41.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.70",
    "vue": "^3.0.7",
    "vue-router": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-roman/vue-simple-acl"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "vue 2",
    "vue3",
    "vue 3",
    "acl",
    "rbac",
    "vue-alc",
    "vue alc",
    "vue-simple-alc",
    "vue simple alc",
    "Access-Control List",
    "Role Based Access Control",
    "javacript",
    "typescript"
  ],
  "author": "Victory Osayi <lah-roman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-roman/vue-simple-acl/issues"
  },
  "homepage": "https://github.com/h-roman/vue-simple-acl#readme"
}
